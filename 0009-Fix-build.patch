diff --git a/src/boost/boost/python/detail/caller.hpp b/src/boost/boost/python/detail/caller.hpp
index 1bd30bfb..bea36479 100644
--- a/src/boost/boost/python/detail/caller.hpp
+++ b/src/boost/boost/python/detail/caller.hpp
@@ -50,7 +50,7 @@ inline PyObject* get(mpl::int_<N>, PyObject* const& args_)
 
 inline Py_ssize_t arity(PyObject* const& args_)
 {
-    return PyTuple_GET_SIZE(args_);
+    return Py_SIZE(args_);
 }
 
 // This "result converter" is really just used as
diff --git a/src/mgr/PythonCompat.h b/src/mgr/PythonCompat.h
index 745dad1a..7bbaf9c4 100644
--- a/src/mgr/PythonCompat.h
+++ b/src/mgr/PythonCompat.h
@@ -13,7 +13,7 @@
 inline PyObject* PyString_FromString(const char *v) {
   return PyUnicode_FromFormat("%s", v);
 }
-inline char* PyString_AsString(PyObject *string) {
+inline const char* PyString_AsString(PyObject *string) {
   return PyUnicode_AsUTF8(string);
 }
 inline long PyInt_AsLong(PyObject *io) {
